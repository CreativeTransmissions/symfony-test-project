{% extends 'base.html.twig' %}

{% block title %}VatRate{% endblock %}

{% block body %}
    <h1>VatRate</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vat_rate.id }}</td>
            </tr>
            <tr>
                <th>Rate</th>
                <td>{{ vat_rate.rate }}</td>
            </tr>
            <tr>
                <th>Effective_date</th>
                <td>{{ vat_rate.effectiveDate ? vat_rate.effectiveDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ vat_rate.createdAt ? vat_rate.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_vat_rate_index') }}">back to list</a>

    <a href="{{ path('app_vat_rate_edit', {'id': vat_rate.id}) }}">edit</a>

    {{ include('vat_rate/_delete_form.html.twig') }}
{% endblock %}
